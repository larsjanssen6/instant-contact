{
    "name": "larsjanssen6/instant-contact",
    "description": ":package_description",
    "keywords": [
        "Contact form",
        "instant-contact",
        "laravel contact form"
    ],
    "homepage": "https://github.com/larsjanssen6/instant-contact",
    "license": "MIT",
    "authors": [
        {
            "name": "Lars Janssen",
            "email": "larsjanssen64@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LarsJanssen\\InstantContact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LarsJanssen\\InstantContact\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LarsJanssen\\InstantContact\\InstantContactServiceProvider"
            ]
        }
    }
}
